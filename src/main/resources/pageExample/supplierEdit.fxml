<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="97.0" prefWidth="217.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pageExample.SupplierEdit">
    <VBox>
        <VBox>
            <HBox>
                <HBox>
                    <Label text="商品名" />
                    <TextField prefHeight="0.0" prefWidth="50" />
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
                </HBox>
                <HBox>
                    <Label text="商品类别" />
                    <TextField prefHeight="0.0" prefWidth="50" />
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
                </HBox>
            </HBox>
        </VBox>
        <VBox prefHeight="45.0" prefWidth="227.0">
            <HBox>
                <HBox>
                    <Label text="数量" />
                    <TextField prefHeight="0.0" prefWidth="50" />
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
                </HBox>
                <HBox>
                    <Label prefHeight="15.0" prefWidth="24.0" text="售价">
                  <HBox.margin>
                     <Insets bottom="10.0" left="13.0" top="10.0" />
                  </HBox.margin></Label>
                    <TextField prefHeight="0.0" prefWidth="50">
                  <HBox.margin>
                     <Insets top="10.0" />
                  </HBox.margin></TextField>
                </HBox>
            <VBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
            </HBox>
        </VBox>
        <HBox prefHeight="13.0" prefWidth="206.0">
            <Button text="增加">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin></Button>
            <Button text="删除">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin></Button>
            <Button fx:id="account_save" onAction="#AccountSave" text="保存修改">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin></Button>
<!--         <VBox.margin>-->
<!--            <Insets bottom="10.0" left="10.0" right="10.0" />-->
<!--         </VBox.margin>-->
        </HBox>
    </VBox>
</AnchorPane>
