<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="150" prefWidth="500" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pageExample.ProductEdit">
    <VBox prefHeight="150.0" prefWidth="500.0">
        <VBox>
            <HBox prefWidth="291.0">
                <HBox>
                    <Label text="商品名">
                  <HBox.margin>
                     <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                  </HBox.margin></Label>
                    <TextField fx:id="customerProduct" prefHeight="0.0" prefWidth="50" />
                </HBox>
                <Label fx:id="customerProductError" prefHeight="15.0" prefWidth="100.0" textFill="RED" />
                <HBox>
                    <Label text="商品类别">
                  <HBox.margin>
                     <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                  </HBox.margin></Label>
                    <TextField fx:id="customerCategory" prefHeight="0.0" prefWidth="50" />
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <padding>
                  <Insets left="30.0" />
               </padding>
                </HBox>
                <Label fx:id="customerCategoryError" prefHeight="15.0" prefWidth="100.0" textFill="RED" />
            <VBox.margin>
               <Insets top="10.0" />
            </VBox.margin>
            </HBox>
        </VBox>
        <VBox>
            <HBox>
                <HBox>
                    <Label text="数量">
                  <HBox.margin>
                     <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                  </HBox.margin></Label>
                    <TextField fx:id="customerAmount" prefHeight="0.0" prefWidth="50" />
                </HBox>
                <Label fx:id="customerAmountError" prefHeight="15.0" prefWidth="100.0" textFill="RED" />
                <HBox>
                    <Label text="进价">
                  <HBox.margin>
                     <Insets bottom="10.0" left="23.0" right="10.0" top="10.0" />
                  </HBox.margin>
                  <padding>
                     <Insets left="30.0" />
                  </padding></Label>
                    <TextField fx:id="customerPrice" prefHeight="0.0" prefWidth="50" />
                </HBox>
                <Label fx:id="customerPriceError" prefHeight="15.0" prefWidth="100.0" textFill="RED" />
            </HBox>
        </VBox>
        <VBox>
            <HBox>
                <Label text="售价">
                    <HBox.margin>
                        <Insets bottom="10.0" left="23.0" right="10.0" top="10.0" />
                    </HBox.margin>
                    <padding>
                        <Insets left="30.0" />
                    </padding></Label>
                <TextField fx:id="SalePrice" prefHeight="0.0" prefWidth="50" />
            </HBox>
            <Label fx:id="SalePriceError" prefHeight="15.0" prefWidth="100.0" textFill="RED" />
        </HBox>
        </VBox>
        <HBox prefWidth="368.0">
            <Button fx:id="product_save" alignment="CENTER" onAction="#productSaveBack" prefHeight="23.0" prefWidth="149.0" text="保存修改">
            <HBox.margin>
               <Insets bottom="10.0" left="150.0" right="10.0" top="10.0" />
            </HBox.margin></Button>
         <VBox.margin>
            <Insets />
         </VBox.margin>
        </HBox>
        <Label fx:id="AddMessage" alignment="CENTER" prefHeight="15.0" prefWidth="150.0" textFill="RED">
            <VBox.margin>
                <Insets left="150.0" />
            </VBox.margin></Label>
        <Label fx:id="AddDBMessage" alignment="CENTER" prefHeight="15.0" prefWidth="100.0" textFill="RED">
         <VBox.margin>
            <Insets left="175.0" />
         </VBox.margin></Label>
    </VBox>
</AnchorPane>
